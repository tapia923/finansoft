@model PlanillajeColectivos.DTO.Contabilidad.comprobante

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Comprobante Informe Diario";
    var fecha = ViewBag.FechaCompro;
}

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<link href="~/Content/css/chosen.css" rel="stylesheet" />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <h3 style="text-align:center"><span class="label label-info">Comprobante Informe Diario</span></h3>
    <div class="row">
        <div class="col-md-4">

        </div>
        <div class="form-group col-md-4">
            <label class="control-label">Fecha de solicitud:</label>
            <input type="text" class="form-control" value="@fecha" readonly />
        </div>
    </div>
    

</div>
}


